<Application x:Class="TicTacToe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ticTacToe="clr-namespace:TicTacToe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <ticTacToe:InvertedBooleanToVisibilityConverter x:Key="InvertetBooleanToVisibilityConverter"/>
        <ticTacToe:WinStatusToColorConverter x:Key="WinStatusToColorConverter"/>
        <ticTacToe:GameStatusToVisibilityConverter x:Key="GameStatusToVisibilityConverter"/>
    </Application.Resources>
</Application>
