<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Tic Tac Toe</title>
        <link rel=stylesheet type=text/css href="{{ url_for('static', filename='style.css') }}">
    </head>
    <body>
        <div class="content">
            <h1>Tic Tac Toe</h1>
            <table class="grid" cellspacing="0">
                <tr class="row">
                    <td class="cell"></td>
                    <td class="cell filled">o</td>
                    <td class="cell filled">x</td>
                </tr>
                <tr class="row">
                    <td class="cell"></td>
                    <td class="cell filled">x</td>
                    <td class="cell filled">o</td>
                </tr>
                <tr class="row">
                    <td class="cell"></td>
                    <td class="cell"></td>
                    <td class="cell"></td>
                </tr>
            </table>
        </div>
    </body>
</html>